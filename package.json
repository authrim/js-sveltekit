{
  "name": "@authrim/sveltekit",
  "packageManager": "pnpm@9.15.0",
  "version": "0.1.2",
  "description": "SvelteKit SDK for Authrim authentication",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./stores": {
      "types": "./dist/stores/index.d.ts",
      "svelte": "./dist/stores/index.js",
      "import": "./dist/stores/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "svelte": "./dist/components/index.js",
      "import": "./dist/components/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "svelte": "./dist/ui/index.js",
      "import": "./dist/ui/index.js"
    },
    "./ui/styles": {
      "import": "./dist/ui/styles/theme.css"
    },
    "./ui/templates": {
      "types": "./dist/ui/templates/index.d.ts",
      "svelte": "./dist/ui/templates/index.js",
      "import": "./dist/ui/templates/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "svelte-kit sync && svelte-package",
    "dev": "vite dev",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src --ext .ts,.svelte",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "clean": "rm -rf dist .svelte-kit",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run typecheck && pnpm run test"
  },
  "keywords": [
    "auth",
    "authentication",
    "oidc",
    "oauth2",
    "pkce",
    "authrim",
    "svelte",
    "sveltekit",
    "passkey",
    "webauthn",
    "social-login",
    "session-management"
  ],
  "author": "Authrim <info@authrim.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/authrim/js-sveltekit.git"
  },
  "homepage": "https://github.com/authrim/js-sveltekit#readme",
  "bugs": {
    "url": "https://github.com/authrim/js-sveltekit/issues"
  },
  "peerDependencies": {
    "@authrim/core": ">=0.1.10",
    "svelte": "^4.0.0 || ^5.0.0",
    "@sveltejs/kit": "^2.0.0"
  },
  "devDependencies": {
    "@authrim/core": "^0.1.10",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.2.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
